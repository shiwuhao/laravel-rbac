{
    "name": "shiwuhao/laravel-rbac",
    "description": "A laravel Role-Based Access Control package",
    "license": "MIT",
    "authors": [
        {
            "name": "shiwuhao",
            "email": "admin@shiwuhao.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shiwuhao\\Rbac\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shiwuhao\\Rbac\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RbacServiceProvider"
            ],
            "aliases": {
                "Rbac": "Shiwuhao\\Rbac\\Rbac"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "require": {
        "maatwebsite/excel": "^3.1"
    }
}
