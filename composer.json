{
  "name": "shiwuhao/laravel-rbac",
  "description": "A laravel Role-Based Access Control package",
  "license": "MIT",
  "authors": [
    {
      "name": "shiwuhao",
      "email": "admin@shiwuhao.com"
    }
  ],
  "require": {
    "illuminate/support": "^5.6",
    "illuminate/database": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "Shiwuhao\\Rbac\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shiwuhao\\Rbac\\ServiceProvider"
      ],
      "aliases": {
        "Rbac": "Shiwuhao\\Rbac\\Rbac"
      }
    }
  },
  "require-dev": {
    "laravel/framework": "^5.8",
    "mockery/mockery": "^1.2",
    "phpunit/phpunit": "^8.0"
  }
}
