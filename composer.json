{
    "name": "shiwuhao/laravel-rbac",
    "description": "A laravel Role-Based Access Control package",
    "license": "MIT",
    "authors": [
        {
            "name": "shiwuhao",
            "email": "admin@shiwuhao.com"
        }
    ],
    "require": {
        "illuminate/console": ">5.5",
        "illuminate/routing": ">5.5",
        "illuminate/support": ">5.5",
        "illuminate/database": ">5.5",
        "illuminate/foundation": ">5.5"
    },
    "autoload": {
        "psr-4": {
            "Shiwuhao\\Rbac\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shiwuhao\\Rbac\\ServiceProvider"
            ],
            "aliases": {
                "Rbac": "Shiwuhao\\Rbac\\Rbac"
            }
        }
    }
}
